<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="16dp">

    <TextView
        android:id="@+id/text_view_jenis_tes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cek Kembali Isian Data"
        android:textSize="25sp"
        android:layout_marginBottom="15dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/text_view_jenis_tes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jenis Tes"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/text_view_rapid_antigen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rapid Antigen"
        android:layout_marginBottom="15dp"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/text_view_tanggal_terkonfirmasi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tanggal Terkonfirmasi"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/text_view_tanggal_terkonfirmasi_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="8 September 2021"
        android:layout_marginBottom="15dp"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/text_view_nik"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NIK"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/text_view_nik_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="7323456778941234"
        android:layout_marginBottom="15dp"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/text_view_nama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nama"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/text_view_nama_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mohammad Reksa"
        android:layout_marginBottom="15dp"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/text_view_tanggal_lahir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tanggal Lahir"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/text_view_tanggal_lahir_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2 November 1993"
        android:layout_marginBottom="15dp"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/text_view_jenis_kelamin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jenis Kelamin"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/text_view_jenis_kelamin_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Laki-laki"
        android:layout_marginBottom="15dp"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/text_view_hubungan_dengan_anda"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hubungan Dengan Anda"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/text_view_hubungan_dengan_anda_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Orangtua"
        android:layout_marginBottom="15dp"
        android:textSize="16sp" />

    <Button
        android:id="@+id/button_cek_hasil_tes"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Cek Hasil Tes"
        android:layout_marginBottom="15dp"/>

    <Button
        android:id="@+id/button_ubah"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Ubah" />

</LinearLayout>
